<script>
  var mySwiper = new Swiper ('.swiper-container', {
    // Optional parameters
    direction: 'horizontal',
    // loop: true,

    // If we need pagination
    // pagination: '.swiper-pagination',
    // autoplay: 4000,
    // speed: 1000,
    initialSlide: 0,
  })


  $("#univ-list-show").on('click',function() {
      $("#univ-item-list").toggleClass("active");
  });

  $("#univ-list-hide").on('click',function() {
      $("#univ-item-list").toggleClass("active");
  });

  $('.collapse').on('shown.bs.collapse', function () {
    $(this).children('.card-block').addClass('active');
  }).on('hide.bs.collapse', function () {
    $(this).children('.card-block').removeClass('active');
  })

  // Smooth scroll

  $('a[href*="#"]:not([href="#"]):not([data-toggle="modal"])').on('click',function() {
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
      var target = $(this.hash);
      target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      if (target.length) {
        $('html, body').animate({
          scrollTop: target.offset().top
        }, 600);
        return false;
      }
    }
  });

  // Close modal with back button

  $('div.modal').on('show.bs.modal', function() {
  	var modal = this;
  	var hash = modal.id;
  	window.location.hash = hash;
  	window.onhashchange = function() {
  		if (!location.hash){
  			$(modal).modal('hide');
  		}
  	}
  });

  $('div.modal').on('hidden.bs.modal', function() {

    $('.collapse').collapse('hide');

  	var hash = this.id;
  	history.pushState('', document.title, window.location.pathname);
  });

</script>


